        ini_write_string(global.consCardName,"Card Name", ""); //writes a bank (0) "Int" stat
        ini_write_real(global.consCardName,"Int", 0); //writes a bank (0) "Int" stat
        ini_write_real(global.consCardName,"Char", 0);//writes a blank (0) "Char" stat
        ini_write_real(global.consCardName,"Well", 0);//writes a blank (0) "Well" stat
        ini_write_real(global.consCardName, "Sprite",-1);//writes a blank (0) "Card1"  
        ini_write_real(global.consCardName, "Info Cards",0);
        ini_close();//closes the ini
    }
    if(global.RoomState == 3)
    {
        global.totalInfoCards += 1; //adds to the TotalCardNum variable
        global.infoCardRef = string(global.totalInfoCards);//creates a string of the curent TotalCardNum
        global.infoCardName = "Info"+global.infoCardRef; //creates the Card+*text of string set above*
        ini_open(global.cardInfoINI);//opens the card ini
