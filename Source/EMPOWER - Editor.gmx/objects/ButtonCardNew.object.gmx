        ini_write_string(global.infoCardName,"Card Name", ""); //writes a bank (0) "Int" stat
        ini_write_string(global.infoCardName,"Card Text", ""); //writes a bank (0) "Int" stat
        ini_write_real(global.infoCardName, "Sprite",-1);//writes a blank (0) "Card1" 
        ini_write_string(global.infoCardName,"Link", ""); //writes a bank (0) "Int" stat
        
        ini_close();//closes the ini
    }
    if(global.RoomState == 4)
    {
        global.totalCharCards += 1; //adds to the TotalCardNum variable
        global.charCardRef = string(global.totalCharCards);//creates a string of the curent TotalCardNum
        global.charCardName = "Char"+global.charCardRef; //creates the Card+*text of string set above*
        ini_open(global.cardCharINI);//opens the card ini
